<mvc:View controllerName="com.lab2dev.firstapp.controller.Home"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <List
		headerText="Products"
		headerLevel="H2"
		items="{listModel>/}" >
		<StandardListItem
			title="{listModel>name}"
			counter="{listModel>quantity}"/>
	</List>
</mvc:View>